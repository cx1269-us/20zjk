#include <stdio.h>
#include <math.h>
#include <stdlib.h>
int main()
{
    float a,b,c,s,area;
    scanf("%f,%f,%f",&a,&b,&c);
    s=(a+b+c)/2;
    if(a+b>c&&b+c>a&&a+c>b)
    {
     area=(float)sqrt(s*(s-a)*(s-b)*(s-c));
     printf("area=%f",area);
    }

   else
   {
       printf("It is not a triangle");
   }
    return 0;
}
